#. Plugin Name of the plugin
msgid "Simple Product Bundles"
msgstr "Lots de Produits Simples"

#. Description of the plugin
msgid "Create product bundles with configurable min/max quantities for each bundled item"
msgstr "Créez des lots de produits avec des quantités min/max configurables pour chaque article"

#: includes/class-simple-product-bundles-admin.php
msgid "Product Bundle"
msgstr "Lot de Produits"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundle Items"
msgstr "Articles du Lot"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundled Products"
msgstr "Produits Groupés"

#: includes/class-simple-product-bundles-admin.php
msgid "Add Product"
msgstr "Ajouter un Produit"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundle Discount"
msgstr "Remise du Lot"

#: includes/class-simple-product-bundles-admin.php
msgid "Applied to the total bundle price"
msgstr "Appliqué au prix total du lot"

#: includes/class-simple-product-bundles-admin.php
msgid "Enable bundle quantity selector"
msgstr "Activer le sélecteur de quantité du lot"

#: includes/class-simple-product-bundles-admin.php
msgid "Allow customers to order multiple bundles at once"
msgstr "Permettre aux clients de commander plusieurs lots à la fois"

#: includes/class-simple-product-bundles-admin.php
msgid "Hide product images"
msgstr "Masquer les images des produits"

#: includes/class-simple-product-bundles-admin.php
msgid "Hide bundled product thumbnails on the product page"
msgstr "Masquer les miniatures des produits groupés sur la page produit"

#: includes/class-simple-product-bundles-admin.php
msgid "Drag to reorder"
msgstr "Glisser pour réorganiser"

#: includes/class-simple-product-bundles-admin.php
msgid "Search for a product..."
msgstr "Rechercher un produit..."

#: includes/class-simple-product-bundles-admin.php
msgid "Remove"
msgstr "Supprimer"

#: includes/class-simple-product-bundles-admin.php
msgid "Min Quantity"
msgstr "Quantité Min"

#: includes/class-simple-product-bundles-admin.php
msgid "0 = optional"
msgstr "0 = optionnel"

#: includes/class-simple-product-bundles-admin.php
msgid "Max Quantity"
msgstr "Quantité Max"

#: includes/class-simple-product-bundles-admin.php
msgid "0 = unlimited"
msgstr "0 = illimité"

#: includes/class-simple-product-bundles-admin.php
msgid "Default Quantity"
msgstr "Quantité par Défaut"

#: includes/class-simple-product-bundles-admin.php
msgid "Pre-selected qty"
msgstr "Qté présélectionnée"

#: includes/class-simple-product-bundles-admin.php
msgid "Volume Discounts"
msgstr "Remises sur Volume"

#: includes/class-simple-product-bundles-admin.php
msgid "Buy"
msgstr "Achetez"

#: includes/class-simple-product-bundles-admin.php
msgid "qty"
msgstr "qté"

#: includes/class-simple-product-bundles-admin.php
msgid "or more"
msgstr "ou plus"

#: includes/class-simple-product-bundles-admin.php
msgid "Get"
msgstr "Obtenez"

#: includes/class-simple-product-bundles-admin.php
msgid "% off"
msgstr "% de réduction"

#: includes/class-simple-product-bundles-admin.php includes/class-simple-product-bundles-cart.php includes/class-simple-product-bundles-frontend.php
msgid "off"
msgstr "de réduction"

#: includes/class-simple-product-bundles-admin.php
msgid "Remove tier"
msgstr "Supprimer le palier"

#: includes/class-simple-product-bundles-admin.php
msgid "Add Tier"
msgstr "Ajouter un Palier"

#: includes/class-simple-product-bundles-admin.php
msgid "Set quantity thresholds with discounts (percentage or fixed amount per item). Higher quantities override lower tiers."
msgstr "Définissez des seuils de quantité avec des remises (pourcentage ou montant fixe par article). Les quantités supérieures remplacent les paliers inférieurs."

#: includes/class-simple-product-bundles-frontend.php includes/class-simple-product-bundles-cart.php
msgid "This bundle has no products configured."
msgstr "Ce lot n'a aucun produit configuré."

#: includes/class-simple-product-bundles-frontend.php
msgid "Bundle Contents"
msgstr "Contenu du Lot"

#: includes/class-simple-product-bundles-frontend.php
msgid "each"
msgstr "pièce"

#: includes/class-simple-product-bundles-frontend.php
msgid "Volume Deals:"
msgstr "Offres Volume :"

#: includes/class-simple-product-bundles-frontend.php
msgid "per item"
msgstr "par pièce"

#: includes/class-simple-product-bundles-frontend.php
msgid "Decrease quantity"
msgstr "Diminuer la quantité"

#: includes/class-simple-product-bundles-frontend.php
msgid "Quantity"
msgstr "Quantité"

#: includes/class-simple-product-bundles-frontend.php
msgid "Increase quantity"
msgstr "Augmenter la quantité"

#: includes/class-simple-product-bundles-frontend.php
msgid "Optional"
msgstr "Optionnel"

#. translators: %d: maximum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "Up to %d"
msgstr "Jusqu'à %d"

#. translators: %d: minimum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "Min %d"
msgstr "Min %d"

#. translators: %1$d: minimum quantity, %2$d: maximum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "%1$d–%2$d qty"
msgstr "%1$d–%2$d pièces"

#: includes/class-simple-product-bundles-frontend.php
msgid "Subtotal"
msgstr "Sous-total"

#: includes/class-simple-product-bundles-frontend.php includes/class-simple-product-bundles-cart.php
msgid "Volume Savings"
msgstr "Économies Volume"

#. translators: %s: discount percentage
#: includes/class-simple-product-bundles-frontend.php
msgid "Bundle Discount (%s%%)"
msgstr "Remise du Lot (%s%%)"

#: includes/class-simple-product-bundles-frontend.php
msgid "Total"
msgstr "Total"

#: includes/class-simple-product-bundles-cart.php
msgid "Security check failed. Please try again."
msgstr "Échec de la vérification de sécurité. Veuillez réessayer."

#: includes/class-simple-product-bundles-cart.php
msgid "Please configure bundle quantities."
msgstr "Veuillez configurer les quantités du lot."

#. translators: %1$s: product name, %2$d: minimum quantity
#: includes/class-simple-product-bundles-cart.php
msgid "Quantity for \"%s\" must be at least %d."
msgstr "La quantité pour \"%s\" doit être d'au moins %d."

#. translators: %1$s: product name, %2$d: maximum quantity
#: includes/class-simple-product-bundles-cart.php
msgid "Quantity for \"%s\" cannot exceed %d."
msgstr "La quantité pour \"%s\" ne peut pas dépasser %d."

#: includes/class-simple-product-bundles-cart.php
msgid "off each"
msgstr "de réduction par pièce"

