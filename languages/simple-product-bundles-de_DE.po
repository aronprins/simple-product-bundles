#. Plugin Name of the plugin
msgid "Simple Product Bundles"
msgstr "Einfache Produktpakete"

#. Description of the plugin
msgid "Create product bundles with configurable min/max quantities for each bundled item"
msgstr "Erstellen Sie Produktpakete mit konfigurierbaren Min/Max-Mengen für jeden gebündelten Artikel"

#: includes/class-simple-product-bundles-admin.php
msgid "Product Bundle"
msgstr "Produktpaket"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundle Items"
msgstr "Paketartikel"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundled Products"
msgstr "Gebündelte Produkte"

#: includes/class-simple-product-bundles-admin.php
msgid "Add Product"
msgstr "Produkt hinzufügen"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundle Discount"
msgstr "Paketrabatt"

#: includes/class-simple-product-bundles-admin.php
msgid "Applied to the total bundle price"
msgstr "Wird auf den Gesamtpreis des Pakets angewendet"

#: includes/class-simple-product-bundles-admin.php
msgid "Enable bundle quantity selector"
msgstr "Paketmengen-Auswahl aktivieren"

#: includes/class-simple-product-bundles-admin.php
msgid "Allow customers to order multiple bundles at once"
msgstr "Kunden erlauben, mehrere Pakete auf einmal zu bestellen"

#: includes/class-simple-product-bundles-admin.php
msgid "Drag to reorder"
msgstr "Ziehen zum Sortieren"

#: includes/class-simple-product-bundles-admin.php
msgid "Search for a product..."
msgstr "Nach einem Produkt suchen..."

#: includes/class-simple-product-bundles-admin.php
msgid "Remove"
msgstr "Entfernen"

#: includes/class-simple-product-bundles-admin.php
msgid "Min Quantity"
msgstr "Mindestmenge"

#: includes/class-simple-product-bundles-admin.php
msgid "0 = optional"
msgstr "0 = optional"

#: includes/class-simple-product-bundles-admin.php
msgid "Max Quantity"
msgstr "Höchstmenge"

#: includes/class-simple-product-bundles-admin.php
msgid "0 = unlimited"
msgstr "0 = unbegrenzt"

#: includes/class-simple-product-bundles-admin.php
msgid "Default Quantity"
msgstr "Standardmenge"

#: includes/class-simple-product-bundles-admin.php
msgid "Pre-selected qty"
msgstr "Vorausgewählte Menge"

#: includes/class-simple-product-bundles-admin.php
msgid "Volume Discounts"
msgstr "Mengenrabatte"

#: includes/class-simple-product-bundles-admin.php
msgid "Buy"
msgstr "Kaufen Sie"

#: includes/class-simple-product-bundles-admin.php
msgid "qty"
msgstr "Stk"

#: includes/class-simple-product-bundles-admin.php
msgid "or more"
msgstr "oder mehr"

#: includes/class-simple-product-bundles-admin.php
msgid "Get"
msgstr "Erhalten Sie"

#: includes/class-simple-product-bundles-admin.php
msgid "% off"
msgstr "% Rabatt"

#: includes/class-simple-product-bundles-admin.php includes/class-simple-product-bundles-cart.php includes/class-simple-product-bundles-frontend.php
msgid "off"
msgstr "Rabatt"

#: includes/class-simple-product-bundles-admin.php
msgid "Remove tier"
msgstr "Stufe entfernen"

#: includes/class-simple-product-bundles-admin.php
msgid "Add Tier"
msgstr "Stufe hinzufügen"

#: includes/class-simple-product-bundles-admin.php
msgid "Set quantity thresholds with discounts (percentage or fixed amount per item). Higher quantities override lower tiers."
msgstr "Legen Sie Mengenschwellen mit Rabatten fest (Prozentsatz oder Festbetrag pro Artikel). Höhere Mengen überschreiben niedrigere Stufen."

#: includes/class-simple-product-bundles-frontend.php includes/class-simple-product-bundles-cart.php
msgid "This bundle has no products configured."
msgstr "Dieses Paket hat keine konfigurierten Produkte."

#: includes/class-simple-product-bundles-frontend.php
msgid "Bundle Contents"
msgstr "Paketinhalt"

#: includes/class-simple-product-bundles-frontend.php
msgid "each"
msgstr "pro Stück"

#: includes/class-simple-product-bundles-frontend.php
msgid "Volume Deals:"
msgstr "Mengenangebote:"

#. translators: %d: minimum quantity, %s: discount amount
#: includes/class-simple-product-bundles-frontend.php
msgid "%d+ = %s off each"
msgstr "%d+ = %s Rabatt pro Stück"

#. translators: %d: minimum quantity, %s: discount percentage
#: includes/class-simple-product-bundles-frontend.php
msgid "%d+ = %s%% off"
msgstr "%d+ = %s%% Rabatt"

#: includes/class-simple-product-bundles-frontend.php
msgid "Decrease quantity"
msgstr "Menge verringern"

#: includes/class-simple-product-bundles-frontend.php
msgid "Quantity"
msgstr "Menge"

#: includes/class-simple-product-bundles-frontend.php
msgid "Increase quantity"
msgstr "Menge erhöhen"

#: includes/class-simple-product-bundles-frontend.php
msgid "Optional"
msgstr "Optional"

#. translators: %d: maximum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "Up to %d"
msgstr "Bis zu %d"

#. translators: %d: minimum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "Min %d"
msgstr "Mind. %d"

#. translators: %1$d: minimum quantity, %2$d: maximum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "%1$d–%2$d qty"
msgstr "%1$d–%2$d Stk"

#: includes/class-simple-product-bundles-frontend.php
msgid "Subtotal"
msgstr "Zwischensumme"

#: includes/class-simple-product-bundles-frontend.php includes/class-simple-product-bundles-cart.php
msgid "Volume Savings"
msgstr "Mengenersparnis"

#. translators: %s: discount percentage
#: includes/class-simple-product-bundles-frontend.php
msgid "Bundle Discount (%s%%)"
msgstr "Paketrabatt (%s%%)"

#: includes/class-simple-product-bundles-frontend.php
msgid "Total"
msgstr "Gesamt"

#: includes/class-simple-product-bundles-cart.php
msgid "Security check failed. Please try again."
msgstr "Sicherheitsüberprüfung fehlgeschlagen. Bitte versuchen Sie es erneut."

#: includes/class-simple-product-bundles-cart.php
msgid "Please configure bundle quantities."
msgstr "Bitte konfigurieren Sie die Paketmengen."

#. translators: %1$s: product name, %2$d: minimum quantity
#: includes/class-simple-product-bundles-cart.php
msgid "Quantity for \"%s\" must be at least %d."
msgstr "Die Menge für \"%s\" muss mindestens %d betragen."

#. translators: %1$s: product name, %2$d: maximum quantity
#: includes/class-simple-product-bundles-cart.php
msgid "Quantity for \"%s\" cannot exceed %d."
msgstr "Die Menge für \"%s\" darf %d nicht überschreiten."

#: includes/class-simple-product-bundles-cart.php
msgid "off each"
msgstr "Rabatt pro Stück"

