#. Plugin Name of the plugin
msgid "Simple Product Bundles"
msgstr "Eenvoudige Productbundels"

#. Description of the plugin
msgid "Create product bundles with configurable min/max quantities for each bundled item"
msgstr "Maak productbundels met configureerbare minimum/maximum hoeveelheden voor elk gebundeld artikel"

#: includes/class-simple-product-bundles-admin.php
msgid "Product Bundle"
msgstr "Productbundel"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundle Items"
msgstr "Bundel artikelen"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundled Products"
msgstr "Gebundelde producten"

#: includes/class-simple-product-bundles-admin.php
msgid "Add Product"
msgstr "Product toevoegen"

#: includes/class-simple-product-bundles-admin.php
msgid "Bundle Discount"
msgstr "Bundelkorting"

#: includes/class-simple-product-bundles-admin.php
msgid "Applied to the total bundle price"
msgstr "Toegepast op de totale bundelprijs"

#: includes/class-simple-product-bundles-admin.php
msgid "Enable bundle quantity selector"
msgstr "Bundelaantal selector inschakelen"

#: includes/class-simple-product-bundles-admin.php
msgid "Allow customers to order multiple bundles at once"
msgstr "Klanten toestaan meerdere bundels tegelijk te bestellen"

#: includes/class-simple-product-bundles-admin.php
msgid "Hide product images"
msgstr "Productafbeeldingen verbergen"

#: includes/class-simple-product-bundles-admin.php
msgid "Hide bundled product thumbnails on the product page"
msgstr "Verberg productminiaturen van gebundelde producten op de productpagina"

#: includes/class-simple-product-bundles-admin.php
msgid "Drag to reorder"
msgstr "Sleep om te herschikken"

#: includes/class-simple-product-bundles-admin.php
msgid "Search for a product..."
msgstr "Zoek een product..."

#: includes/class-simple-product-bundles-admin.php
msgid "Remove"
msgstr "Verwijderen"

#: includes/class-simple-product-bundles-admin.php
msgid "Min Quantity"
msgstr "Min. aantal"

#: includes/class-simple-product-bundles-admin.php
msgid "0 = optional"
msgstr "0 = optioneel"

#: includes/class-simple-product-bundles-admin.php
msgid "Max Quantity"
msgstr "Max. aantal"

#: includes/class-simple-product-bundles-admin.php
msgid "0 = unlimited"
msgstr "0 = onbeperkt"

#: includes/class-simple-product-bundles-admin.php
msgid "Default Quantity"
msgstr "Standaard aantal"

#: includes/class-simple-product-bundles-admin.php
msgid "Pre-selected qty"
msgstr "Voorgeselecteerd aantal"

#: includes/class-simple-product-bundles-admin.php
msgid "Volume Discounts"
msgstr "Staffelkortingen"

#: includes/class-simple-product-bundles-admin.php
msgid "Buy"
msgstr "Koop"

#: includes/class-simple-product-bundles-admin.php
msgid "qty"
msgstr "aantal"

#: includes/class-simple-product-bundles-admin.php
msgid "or more"
msgstr "of meer"

#: includes/class-simple-product-bundles-admin.php
msgid "Get"
msgstr "Krijg"

#: includes/class-simple-product-bundles-admin.php
msgid "% off"
msgstr "% korting"

#: includes/class-simple-product-bundles-admin.php includes/class-simple-product-bundles-cart.php includes/class-simple-product-bundles-frontend.php
msgid "off"
msgstr "korting"

#: includes/class-simple-product-bundles-admin.php
msgid "Remove tier"
msgstr "Staffel verwijderen"

#: includes/class-simple-product-bundles-admin.php
msgid "Add Tier"
msgstr "Staffel toevoegen"

#: includes/class-simple-product-bundles-admin.php
msgid "Set quantity thresholds with discounts (percentage or fixed amount per item). Higher quantities override lower tiers."
msgstr "Stel aantaldrempels in met kortingen (percentage of vast bedrag per artikel). Hogere aantallen overschrijven lagere staffels."

#: includes/class-simple-product-bundles-frontend.php includes/class-simple-product-bundles-cart.php
msgid "This bundle has no products configured."
msgstr "Deze bundel heeft geen producten geconfigureerd."

#: includes/class-simple-product-bundles-frontend.php
msgid "Bundle Contents"
msgstr "Bundelinhoud"

#: includes/class-simple-product-bundles-frontend.php
msgid "each"
msgstr "per stuk"

#: includes/class-simple-product-bundles-frontend.php
msgid "Volume Deals:"
msgstr "Staffeldeals:"

#: includes/class-simple-product-bundles-frontend.php
msgid "per item"
msgstr "per stuk"

#: includes/class-simple-product-bundles-frontend.php
msgid "Decrease quantity"
msgstr "Aantal verlagen"

#: includes/class-simple-product-bundles-frontend.php
msgid "Quantity"
msgstr "Aantal"

#: includes/class-simple-product-bundles-frontend.php
msgid "Increase quantity"
msgstr "Aantal verhogen"

#: includes/class-simple-product-bundles-frontend.php
msgid "Optional"
msgstr "Optioneel"

#. translators: %d: maximum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "Up to %d"
msgstr "Tot %d"

#. translators: %d: minimum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "Min %d"
msgstr "Min. %d"

#. translators: %1$d: minimum quantity, %2$d: maximum quantity
#: includes/class-simple-product-bundles-frontend.php
msgid "%1$d–%2$d qty"
msgstr "%1$d–%2$d stuks"

#: includes/class-simple-product-bundles-frontend.php
msgid "Subtotal"
msgstr "Subtotaal"

#: includes/class-simple-product-bundles-frontend.php includes/class-simple-product-bundles-cart.php
msgid "Volume Savings"
msgstr "Staffelkorting"

#. translators: %s: discount percentage
#: includes/class-simple-product-bundles-frontend.php
msgid "Bundle Discount (%s%%)"
msgstr "Bundelkorting (%s%%)"

#: includes/class-simple-product-bundles-frontend.php
msgid "Total"
msgstr "Totaal"

#: includes/class-simple-product-bundles-cart.php
msgid "Security check failed. Please try again."
msgstr "Beveiligingscontrole mislukt. Probeer het opnieuw."

#: includes/class-simple-product-bundles-cart.php
msgid "Please configure bundle quantities."
msgstr "Configureer de bundelaantallen."

#. translators: %1$s: product name, %2$d: minimum quantity
#: includes/class-simple-product-bundles-cart.php
msgid "Quantity for \"%s\" must be at least %d."
msgstr "Het aantal voor \"%s\" moet minimaal %d zijn."

#. translators: %1$s: product name, %2$d: maximum quantity
#: includes/class-simple-product-bundles-cart.php
msgid "Quantity for \"%s\" cannot exceed %d."
msgstr "Het aantal voor \"%s\" mag niet meer dan %d zijn."

#: includes/class-simple-product-bundles-cart.php
msgid "off each"
msgstr "korting per stuk"

